@article{Haerdle1989,
abstract = {Let (x1,...,xk, y) be a random vector where y denotes a response on the vector x of predictor variables. In this article we propose a technique [termed average derivative estimation (ADE)] for studying the mean response m(x) = E(y∣ x) through the estimation of the k vector of average derivatives $\delta$ = E(m'). The ADE procedure involves two stages: first estimate $\delta$ using an estimator {\$}\backslashhat\backslashdelta{\$} , and then approximate m(x) by {\$}\backslashhat{\{}m{\}}(x) = \backslashhat{\{}g{\}}(x{\^{}}T\backslashhat\backslashdelta){\$} , where ĝ is an estimator of the univariate regression of y on {\$}x{\^{}}T\backslashhat\backslashdelta{\$} . We argue that the ADE procedure exhibits several attractive characteristics: data summarization through interpretable coefficients, graphical depiction of the possible nonlinearity between y and {\$}x{\^{}}T\backslashhat\backslashdelta{\$} , and theoretical properties consistent with dimension reduction. We motivate the ADE procedure using examples of models that take the form m(x) = g̃(xT$\beta$). In this framework, $\delta$ is shown to be proportional to $\beta$ and m̂(x) infers m(x) exactly. The focus of the procedure is on the estimator {\$}\backslashhat\backslashdelta{\$} , which is based on a simple average of kernel smoothers and is shown to be a {\$}\backslashsqrt N{\$} consistent and asymptotically normal estimator of $\delta$. The estimator ĝ ({\textperiodcentered}) is a standard kernel regression estimator and is shown to have the same properties as the kernel regression of y on xT$\delta$. In sum, the estimator {\$}\backslashhat\backslashdelta{\$} converges to $\delta$ at the rate typically available in parametric estimation problems, and m̂(x) converges to E(y∣T$\delta$) at the optimal one-dimensional nonparametric rate. We also give a consistent estimator of the asymptotic covariance matrix of {\$}\backslashhat\backslashdelta{\$} , to facilitate inference. We discuss the conditions underlying these results, including how {\$}\backslashsqrt N{\$} consistent estimation of {\$}\backslashhat\backslashdelta{\$} requires undersmoothing relative to pointwise multivariate estimation. We also indicate the relationship between the ADE method and projection pursuit regression. For illustration, we apply the ADE method to data on automobile collisions.},
author = {Haerdle, Wolfgang and Stoker, Thomas},
doi = {10.2307/2290074},
issn = {01621459},
journal = {Journal of the American Statistical Association},
keywords = {ade regression},
number = {408},
pages = {986--995},
title = {{Investigating Smooth Multiple Regression by the Method of Average Derivatives}},
url = {http://www.jstor.org/stable/2290074{\%}5Cnhttp://www.jstor.org/stable/pdfplus/2290074.pdf?acceptTC=true},
volume = {84},
year = {1989}
