eprint = {0811.1067},
file = {:home/stefan/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Jiang et al. - 2011 - Statistical ranking and combinatorial Hodge theory.pdf:pdf},
isbn = {0011051000},
issn = {00255610},
keywords = {Borda count,Combinatorial Hodge theory,Combinatorial Laplacian,Discrete exterior calculus,Graph Helmholtzian,Hodge Laplacian,HodgeRank,Kemeny optimization,Rank aggregation,Statistical ranking},
number = {1},
pages = {203--244},
title = {{Statistical ranking and combinatorial Hodge theory}},
