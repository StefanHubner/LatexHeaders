journal = {Econometrica},
keywords = {Irregular identification,inverse weighting,rate adaptive inference,rates of convergence},
month = {nov},
number = {6},
pages = {2021--2042},
publisher = {The Econometric Society},
title = {{Irregular Identification, Support Conditions, and Inverse Weight Estimation}},
volume = {78},
year = {2010}
